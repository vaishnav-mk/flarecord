{
  "name": "flarecord",
  "version": "0.0.2",
  "description": "discord gateway client for cloudflare workers. built with typescript and optimized for durable objects.",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./src": {
      "source": "./src/index.ts",
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./src/*": {
      "source": "./src/*.ts",
      "types": "./src/*.ts",
      "import": "./src/*.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaishnav-mk/flarecord.git"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "discord-gateway",
    "cloudflare",
    "cloudflare-workers",
    "durable-objects",
    "websocket",
    "typescript"
  ],
  "author": "vaishnav-mk",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "discord-api-types": "^0.38.37"
  }
}
